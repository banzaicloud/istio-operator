{
  "name": "istio-gke",
  "location": "europe-west4-a",
  "cloud": "google",
  "secretId": "{{secretID}}",
  "properties": {
    "gke": {
      "master": {
        "version": "1.17"
      },
      "nodeVersion": "1.17",
      "nodePools": {
        "pool1": {
          "count": 1,
          "autoscaling": true,
          "instanceType": "n1-standard-2",
          "preemptible": true,
          "minCount": 1,
          "maxCount": 2
        },
        "system": {
          "count": 1,
          "autoscaling": false,
          "instanceType": "n1-standard-2",
          "preemptible": false
        }
      },
      "projectId": "{{projectID}}"
    }
  }
}
